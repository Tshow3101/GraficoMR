            @*['Victoria', @item.Victoria.ToString().Replace(",", "."), 'color: #b3b3ff'],*@
            @*['Olga', @item.Olga, 'color: #cebbff'],*@
	        @*['FERNANDA', @item.Fernanda.ToString().Replace(",", "."), 'color: #4CAF50'],*@
            @*['Arlette', @item.Arlette, 'color: #46af2c'],*@
            @*['Edith', @item.Edith, 'color: #fe7b2e'],*@
            ['Sharon', @item.Sharon.ToString().Replace(",", "."), 'color: #cc0044'],
            ['Eliana', @item.Eliana.ToString().Replace(",", "."), 'color: #fff400'],
            ['María', @item.Maria.ToString().Replace(",", "."), 'color: #79a6d2'],
            ['Marianne', @item.Marianne.ToString().Replace(",", "."), 'color: #FE2EF7'],
            ['Alejandra', @item.Alejandra.ToString().Replace(",", "."), 'color: #8ef7ff'],
            ['Yolanda', @item.Yolanda.ToString().Replace(",", "."), 'color: #e65c00'],
            @*['Renzo', @item.Renzo.ToString().Replace(",", "."), 'color: #de65ff'],*@
            @*['Lola', @item.Lola.ToString().Replace(",", "."), 'color: #A845FF'],*@
            @*['Joan', @item.Joan.ToString().Replace(",", "."), 'color: #cebbff'],*@
            ['Jesus', @item.Jesus.ToString().Replace(",", "."), 'color: #a3f2a1'],
            ['Alejandro', @item.Alejandro.ToString().Replace(",", "."), 'color: #AA6739'],
            ['Ingrid', @item.Ingrid.ToString().Replace(",", "."), 'color: #A845FF'],
            ['Vilma', @item.Vilma.ToString().Replace(",", "."), 'color: #cebbff'],
            ['Lizzy', @item.Lizzy.ToString().Replace(",", "."), 'color: #ED058B']
        ]);

        data.sort([{ column: 1, desc: true }]);

        var formatter = new google.visualization.NumberFormat({
            pattern: "#,###"
        });

        formatter.format(data, 1);

        @*var Ventas='{ label: "GIULIANA", value:'+ @item.Guiliana+', color: colorGiulian },'
		+'{ label: "MARIA", value:'+ @item.Maria+', color: colorGrupos } ,'
